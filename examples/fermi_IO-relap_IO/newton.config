# Numerical methods

METHOD explicit_parallel
# delta step in days
N_STEPS 10 DELTA_STEP 100.0
PHASES fermi & relap &
ABS_TOL 1e-04 MAX_ITER 5
DX_JAC_CALC 0.01
STEPS_JAC_CALC 0
________________________________________________________________________

# Client code specifications

CLIENT relap
INPUT_NAME core
INPUT_EXT i
RESTART_NAME from_input
RESTART_EXT r
RESTART_PATH 
/run/user/1738/gvfs/smb-share:server=10.73.96.200,share=archivosrelap7/
OUTPUT_NAME from_input
OUTPUT_EXT o
OUTPUT_PATH 
/run/user/1738/gvfs/smb-share:server=10.73.96.200,share=archivosrelap7/
CONNECTION io
BIN_COMMAND wine wineconsole run.bat
ARGS input_name
IO_TYPE RELAP_POW2TH

GUESSES
  fpow_z1
  fpow_z2
  fpow_z3
  fpow_z4
  fpow_z5

CALCS
  tref_z1 tfuel_z1 void_z1
  tref_z2 tfuel_z2 void_z2
  tref_z3 tfuel_z3 void_z3
  tref_z4 tfuel_z4 void_Z4
  tref_z5 tfuel_z5 void_z5

X_INI
fpow_z1  0.2
fpow_z2  0.2
fpow_z3  0.2
fpow_z4  0.2
fpow_z5  0.2

T_0 0
T_F 100
MAX_DT 0.1
N_AXIAL_ZONES 5
N_RADIAL_POINTS 11
PIPE_ZONES 120-010000 120-020000 120-030000 120-040000 120-050000
HS_ZONES 1200-001 1200-002 1200-003 1200-004 1200-005  
________________________________________________________________________

CLIENT fermi
# Fail running MPI_Spawn
#SPAWN_TYPE mpi
N_PROCS 1
INPUT_NAME fuel_5
INPUT_EXT fer
OUTPUT_NAME pow_phys
OUTPUT_EXT dat
CONNECTION io
BIN_COMMAND /users/cacciaf/fermi/fermi
ARGS input
IO_TYPE FERMI_XS2POW

GUESSES 
  tref_z1 tfuel_z1 void_z1
  tref_z2 tfuel_z2 void_z2
  tref_z3 tfuel_z3 void_z3
  tref_z4 tfuel_z4 void_Z4
  tref_z5 tfuel_z5 void_z5

CALCS
  fpow_z1
  fpow_z2
  fpow_z3
  fpow_z4
  fpow_z5

X_INI
tref_z1  450
tfuel_z1 305
void_z1  674.83
tref_z2  450
tfuel_z2 305
void_z2  674.83
tref_z3  450
tfuel_z3 305
void_z3  674.83
tref_z4  450
tfuel_z4 305
void_Z4  674.83
tref_z5  450
tfuel_z5 305
void_z5  674.83

N_PHYSICAL_ENTITIES 10
N_GROUPS 1
N_XS 4
PHYSICAL_ENTITIES
"fuel_z1" 
"fuel_z2" 
"fuel_z3" 
"fuel_z4" 
"fuel_z5" 
"fluid_z1"
"fluid_z2"
"fluid_z3"
"fluid_z4"
"fluid_z5"
              
________________________________________________________________________

# Mappers

MAPPER th2xs  
MAP_CLIENT fermi        
STAGE pre_guess
GUESSES_MAPPED 40
# Specifications to th2xs
ENERGY_PER_FISSION 0.4 0.4 0.4 0.4 0.4
MAP_DATA TABLAS.CRM
DATA_N_GROUPS 5
MATERIAL    'E=1.8%/0VQ' &
N_BURNUP 3 PHYS_ENT "fuel_z1" "fuel_z2" "fuel_z3" "fuel_z4" "fuel_z5"
MATERIAL 'Reflector inferior (agua)' &
N_BURNUP 1 PHYS_ENT "fluid_z1" "fluid_z2" "fluid_z3" "fluid_z4" "fluid_z5"

MAPPER pow2fpow
MAP_CLIENT fermi
STAGE post_calc
CALCS_PRE_MAP 5
